{
  "name": "securevault-docs",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=20.11"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc -p . --noEmit",
    "env:switch:dev": "node scripts/switch-env.mjs dev",
    "env:switch:staging": "node scripts/switch-env.mjs staging",
    "env:switch:prod": "node scripts/switch-env.mjs prod",
    "seed:demo": "node scripts/seed-demo.js",
    "set-trial": "npx tsx scripts/set-trial.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
