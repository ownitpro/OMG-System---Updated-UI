        "responseBody": "={\"ok\":true,\"installed\":{{$('If Dry Run').item.json.mode !== 'dry-run'}},\"actions\":{{JSON.stringify($('Dry Run Mapper').item.json.actions || [])}}}",
        "options": {}
      },
