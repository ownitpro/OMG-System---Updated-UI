{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Demo Request Payload",
  "type": "object",
  "required": ["name", "email", "side"],
  "properties": {
    "name": {"type": "string"},
    "email": {"type": "string", "format": "email"},
    "side": {"type": "string", "enum": ["business", "personal"]},
    "orgName": {"type": ["string", "null"]},
    "notes": {"type": ["string", "null"]}
  }
}

